#include<iostream>
#include<vector>
using namespace std;

void ss(vector<int>&arr){
    int n = arr.size();
    for(int i = 0; i<n-1; i++){
        int MI = i; 
for(int j = i+1; j<n; j++){
    if (arr[j] < arr [MI]){
    MI = j;
}
}
swap (arr[i], arr[MI]);
}}
int main(){
    vector<int>arr ={ 64, 25, 12, 22, 11};
    ss(arr);
    for(int num : arr){
    cout<<num<<" ";
}
    return 0;
}
